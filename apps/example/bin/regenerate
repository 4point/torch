#!/usr/bin/env bash

rm -rf web/templates/admin/author && rm -rf web/controllers/admin/author_controller.ex && rm -rf web/views/admin/author_view.ex
rm -rf web/templates/admin/post && rm -rf web/controllers/admin/post_controller.ex && rm -rf web/views/admin/post_view.ex
